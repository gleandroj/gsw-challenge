{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./src/index.js",
    "test": "./node_modules/.bin/mocha --require @babel/register ./tests/**/*.js --exit",
    "dev": "./node_modules/.bin/nodemon --exec babel-node --presets @babel/preset-env ./src/index.js",
    "server:prod": "node ./dist/index.js",
    "clean": "rm -rf dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist",
    "prod": "export NODE_ENV=production && npm run clean && npm run build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "mongodb": "^3.3.5",
    "mongoose": "^5.7.13",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/register": "^7.7.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.2"
  }
}
